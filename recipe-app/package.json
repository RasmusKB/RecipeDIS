  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.15.5",
    "@mui/system": "5.15.15",
    "@types/react": "18.0.35",
    "abortcontroller-polyfill": "1.7.5",
